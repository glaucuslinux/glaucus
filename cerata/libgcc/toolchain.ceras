prepare() {
  cd $TBLD/gcc &&

  $MAKE \
    -C $TGT/$nom distclean
} &&

configure() {
  :
} &&

build() {
  $MAKE \
    all-target-$nom
} &&

install() {
  $MAKE \
    install-strip-target-$nom
}
